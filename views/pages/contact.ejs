<%- include('../partials/header') %>
    <main>
        <%- include('../partials/loader') %>
            <div id="smooth-wrapper">
                <div id="smooth-content">
                    <%- include('../partials/navbar') %>
                        <section class="breadcrumb">
                            <div class="breadcrumb-shape"></div>
                            <div class="container">
                                <div class="row justify-content-center">
                                    <div class="col-lg-12">
                                        <div class="breadcrumb__wrapper">
                                            <ul class="breadcrumb__list">
                                                <li class="breadcrumb__item">
                                                    <a href="index-2.html"> nexgen</a>
                                                </li>
                                                <li class="breadcrumb__item">
                                                    <span class="material-symbols-outlined">
                                                chevron_right
                                            </span>
                                                </li>
                                                <li class="breadcrumb__item">
                                                    <span class="breadcrumb__item-text"> Contacts </span>
                                                </li>
                                            </ul>
                                            <h1 class="breadcrumb__title"> Contacts Us</h1>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>

                        <section class="contacts-section py-120">
                            <div class="container">
                                <div class="row justify-content-center">
                                    <div class="col-xl-6 col-lg-7">
                                        <div class="section-header-two text-center">
                                            <h6>Your global link to high-impact telecom partnership</h6>
                                            <p style="color: black;">Discuss how we can provide to build, operate, and accelerate ypur market presence. Reach out today<p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-gap-4 mt-60">

                                    <div class="col-xl-12">
                                        <div class="contacts-section__item ">
                                            <form action="/contact-submit " method="POST">
                                                <div class="row row-gap-4 ">
                                                    <div class="col-sm-6 ">
                                                        <div class="input-group ">
                                                            <label for="inputName " class="form-label ">First Name</label>
                                                            <input type="text " class="form-control " id="inputName " placeholder="First Name " required>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6 ">
                                                        <div class="input-group ">
                                                            <label for="inputNameLast " class="form-label ">Last Name</label>
                                                            <input type="text " class="form-control " id="inputNameLast " placeholder="Last Name " required>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6 ">
                                                        <div class="input-group ">
                                                            <label for="inputEmail " class="form-label ">Your Email</label>
                                                            <input type="email " class="form-control " id="inputEmail " placeholder="name@example.com " required>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6 ">
                                                        <div class="input-group ">
                                                            <label for="inputPhone " class="form-label ">Your Phone</label>
                                                            <input type="tel " class="form-control " id="inputPhone " placeholder="Your Phone " required>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 ">
                                                        <div class="input-group ">
                                                            <label for="inputAddress " class="form-label ">Your Address</label>
                                                            <input type="text " class="form-control " id="inputAddress " placeholder="Your Address " required>
                                                        </div>
                                                    </div>

                                                    <div class="col-12 ">
                                                        <div class="input-group ">
                                                            <label for="inputMessage " class="form-label ">Message</label>
                                                            <textarea class="form-control " id="inputMessage " placeholder="Message... "></textarea>
                                                        </div>
                                                    </div>

                                                    <div class="col-12 ">
                                                        <button type="submit " class="btn-round-two ">
                                                    Submit
                                                </button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>

                        <%- include('../partials/footer') %>
                </div>
            </div>
    </main>
    <%- include('../partials/footer-include') %>